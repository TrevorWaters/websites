{
  "name": "fostereddesigns",
  "version": "1.0.0",
  "description": "Fostered Designs Web Consulting and Solutions",
  "main": "index.html",
  "dependencies": {
    "alpinejs": "^2.8.1",
    "autoprefixer": "^10.2.4",
    "postcss": "^8.2.4",
    "postcss-cli": "^8.3.1",
    "tailwindcss": "^2.0.2",
    "webpack": "^5.24.2"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "webpack-cli": "^4.5.0"
  },
  "scripts": {
    "build-css-dev": "tailwindcss build ./wwwroot/css/tailwind.css -o ./wwwroot/css/tailwind.min.css -c tailwind.config.js",
    "build-css-prod": "cross-env NODE_ENV=production tailwindcss build ./wwwroot/css/tailwind.css -o ./wwwroot/css/tailwind.min.css -c tailwind.config.js",
    "build-js": "webpack ./wwwroot/js/index.js --output-path ./wwwroot/js --output-filename index.bundle.js"
  },
