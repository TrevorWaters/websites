{
  "name": "kristinsite",
  "version": "1.0.0",
  "description": "Kristin's Store",
  "main": "index.html",
  "dependencies": {
    "alpinejs": "^2.8.1",
    "autoprefixer": "^10.2.5",
    "postcss": "^8.2.8",
    "postcss-cli": "^8.3.1",
    "tailwindcss": "^2.0.4",
    "webpack": "^5.4.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "webpack-cli": "^4.5.0"
  },
  "scripts": {
    "build-css-dev": "tailwindcss build ./Client/wwwroot/css/tailwind.css -o ./Client/wwwroot/css/tailwind.min.css -c tailwind.config.js",
    "build-css-prod": "cross-env NODE_ENV=production tailwindcss build ./Client/wwwroot/css/tailwind.css -o ./Client/wwwroot/css/tailwind.min.css -c tailwind.config.js",
    "build-js": "webpack ./Client/wwwroot/js/index.js --output-path ./Client/wwwroot/js --output-filename index.bundle.js"
  },
  "author": "Trevor Waters",
  "license": "ISC"
}
